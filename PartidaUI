	public void iniciaPartida(){
		System.out.println("Empezando partida");
		//crear personajes 
		// Personaje h1 = new Personaje();
		for (x=1;x<7;x++){
			limpiaMensajes();
			System.out.println("\ninicia ronda " + x);
			iniciaRonda();
		}
		
	public void iniciaRonda(){
		// "personaje" en la coleccion de personajes (tres personajes)
			System.out.println("\n inicia turno de "+personaje);
			iniciaTurno();
		}
	}
	
	public void iniciaTurno(){
